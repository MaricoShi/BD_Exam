    bool isShowPro = true;
}

<div class="pd_tb_10 pd_lr_15">
    <div style="min-height:400px;">
        <h3 class="text-center mg_t_10">
            人员基本信息登记表
        </h3>
        @Html.HiddenFor(p => p.PersonId)

        <table class="table-s02 mg_b_10 text-center" cellpadding="0" cellspacing="0"
               style="width:100%;">
            <thead>
                <tr>
                    <th width="90px">专业</th>
                    <th width="120px">姓名</th>
                    <th>原因</th>
                    <th>备注</th>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>

    </div>
</div>
